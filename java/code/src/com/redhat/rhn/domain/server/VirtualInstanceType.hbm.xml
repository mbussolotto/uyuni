<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE hibernate-mapping
PUBLIC "-//Hibernate/Hibernate Mapping DTD 3.0//EN"
"http://www.hibernate.org/dtd/hibernate-mapping-3.0.dtd">

<hibernate-mapping>
    <query name="VirtualInstanceType.findByLabel">
        <![CDATA[from com.redhat.rhn.domain.server.VirtualInstanceType as vit where vit.label = :label]]>
    </query>
</hibernate-mapping>
